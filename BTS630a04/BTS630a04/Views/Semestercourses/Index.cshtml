@model IEnumerable<BTS630a04.Models.Semestercourses>

@{
    ViewBag.Title = "View All Associations Between Semesters And Courses";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Semester.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Course.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Semester.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Course.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SemestercoursesID }) |
            @Html.ActionLink("Details", "Details", new { id=item.SemestercoursesID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SemestercoursesID })
        </td>
    </tr>
}

</table>
