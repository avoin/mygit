@model IEnumerable<BTS630a04.Models.Professorcredentials>

@{
    ViewBag.Title = "View All Professor Credentials";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Full Name
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Credentials.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Professor.FirstName)&nbsp;
            @Html.DisplayFor(modelItem => item.Professor.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Credentials.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ProfessorCredentialsID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ProfessorCredentialsID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ProfessorCredentialsID })
        </td>
    </tr>
}

</table>
